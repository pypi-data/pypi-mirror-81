[run]
branch = True
source = thepipe

[report]
include =
    thepipe/*
omit =
    */tests/*
    */*/tests/*
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
    self.log.debug
    self.log.info
